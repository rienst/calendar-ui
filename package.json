{
  "name": "calendar-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "build": "npm run build --workspaces --if-present",
    "core:test": "npm run test --workspace @calendar-ui/core",
    "angular:dev": "npm run dev --workspace @calendar-ui/angular",
    "react:dev": "npm run dev --workspace @calendar-ui/react"
  }
}
